# Jira Configuration
# Copy this file to .env and fill in your actual values

# Jira instance URL (required)
JIRA_URL=https://your-jira-instance.atlassian.net

# Jira user email (required)
JIRA_EMAIL=your-email@example.com

# Jira API token (required)
# Get your API token from: https://id.atlassian.com/manage-profile/security/api-tokens
JIRA_API_TOKEN=your-api-token-here

# Project key (default: ZYN)
PROJECT_KEY=ZYN

# ERP Activity filter value (default: ProjectTask-00000007118797)
ERP_ACTIVITY_FILTER=ProjectTask-00000007118797

# Log level: 1=standard, 2=debug (default: 1)
LOG_LEVEL=1
